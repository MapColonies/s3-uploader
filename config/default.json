{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": true
    }
  },
  "server": {
    "port": "8087",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "s3": {
    "awsAccessKeyId": "minioadmin",
    "awsSecretAccessKey": "minioadmin",
    "endPoint": "http://127.0.0.1:9000",
    "forcePathStyle": true,
    "bucket": "3dtiles",
    "destinationBucket": "3dtiles-destination"
  },
  "3dir": "../3D-Models",
  "source": "S3",
  "buffer": 20
}
